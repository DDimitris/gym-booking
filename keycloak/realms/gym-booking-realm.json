{
  "id": "gym-booking",
  "realm": "gym-booking",
  "enabled": true,
  "registrationAllowed": true,
  "displayName": "Gym Booking Application",
  "displayNameHtml": "Gym Booking Application",
  "roles": {
    "realm": [
      { "name": "ADMIN", "description": "Administrator role" },
      { "name": "TRAINER", "description": "Gym trainer role" },
      { "name": "MEMBER", "description": "Gym member role" }
    ]
  },
  "clients": [
    {
      "clientId": "gym-booking-client",
      "enabled": true,
      "publicClient": true,
      "redirectUris": [
        "https://192.168.1.113",
        "https://192.168.1.113/",
        "https://192.168.1.113/*",
        "https://192.168.1.113/classes"
      ],
      "webOrigins": [
        "https://192.168.1.113"
      ],
      "defaultClientScopes": [
        "web-origins",
        "acr",
        "roles",
        "profile",
        "email"
      ],
  "rootUrl": "https://192.168.1.113/",
      "baseUrl": "/",
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": false,
      "frontchannelLogout": true,
      "fullScopeAllowed": true,
      "attributes": {
        "pkce.code.challenge.method": "S256",
        "pkce.enforced": "true",
        "post.logout.redirect.uris": "https://192.168.1.113/*",
        "oauth2.device.authorization.grant.enabled": "false"
      },
      "protocol": "openid-connect",
      "protocolMappers": [
        {
          "name": "roles",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-realm-role-mapper",
          "consentRequired": false,
          "config": {
            "multivalued": "true",
            "userinfo.token.claim": "true",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "roles",
            "jsonType.label": "String"
          }
        }
      ]
    }
  ],
  "users": [
    {
      "username": "admin",
      "enabled": true,
      "credentials": [{ "type": "password", "value": "admin", "temporary": true }],
      "requiredActions": ["UPDATE_PASSWORD"],
      "realmRoles": ["ADMIN"]
    },
    {
      "username": "trainer",
      "enabled": true,
      "credentials": [{ "type": "password", "value": "trainer", "temporary": false }],
      "realmRoles": ["TRAINER"]
    },
    {
      "username": "member",
      "enabled": true,
      "credentials": [{ "type": "password", "value": "member", "temporary": false }],
      "realmRoles": ["MEMBER"]
    }
  ]
}
